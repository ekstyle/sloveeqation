{
    "extends": "tslint-config-airbnb",
    "rules": {
      "max-line-length": {
        "options": [160]
      },
      "align": [true, "parameters", "statements"],
      "import-name": [
        false
      ],
      "variable-name": [false],
      "no-boolean-literal-compare":false,
      "prefer-template": [true, "allow-single-concat"],
      "no-increment-decrement":[false]
    }
  }